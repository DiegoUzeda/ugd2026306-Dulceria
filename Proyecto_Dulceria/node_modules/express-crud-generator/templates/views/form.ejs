<div class="row">


</div class="col-sm-12">

  <form action="<%= Model.url %>/save" method="post" >
    <% Object.keys(Model.schema).forEach(function(property){ %>

      <div class="form-group" >
        <label for=""><%= property %></label>
        <% var _type  = "text"; %>

        <input name="<%= property %>" type="text" value="<%%= item['<%= property %>'] %%>" class="form-control" placeholder="<%= property %>" />
      </div>
    <% }) %>
    <div class="row">
        <a href="" class="btn btn-default" >Cancelar</a>
        <input type="submit" class="btn btn-success" value="&nbsp;Guardar&nbsp;" />

    </div>
  </form>

</div>

</div>
